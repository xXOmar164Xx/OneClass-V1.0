<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" prefHeight="600.0" prefWidth="800.0" spacing="5" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.DashboardElements.RegisterController">
    <padding>
        <Insets topRightBottomLeft="10" />
    </padding>
    <HBox spacing="5">
        <DatePicker fx:id="datePicker" />
      <ChoiceBox fx:id="classPicker" value="Q">
          <items>
              <FXCollections fx:factory="observableArrayList">
                  <String fx:value="Q" />
                  <String fx:value="P" />
              </FXCollections>
          </items>
      </ChoiceBox>
        <Button text="Reset Changes" fx:id="resetButton"/>
    </HBox>
    <HBox spacing="5">
        <TableView fx:id="table" editable="true" prefHeight="400.0" prefWidth="614.0" HBox.hgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="nameCol" sortType="DESCENDING" text="Name">
                    <cellValueFactory>
                        <PropertyValueFactory property="name" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="Monday">
                    <cellValueFactory>
                        <PropertyValueFactory property="monday" />
                    </cellValueFactory>
                    <columns>
                        <TableColumn text="P1">
                            <cellValueFactory>
                                <PropertyValueFactory property="mp1" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn text="P2">
                            <cellValueFactory>
