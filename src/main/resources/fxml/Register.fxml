                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn text="P5">
                            <cellValueFactory>
                                <PropertyValueFactory property="mp5" />
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                </TableColumn>
                <TableColumn text="Tuesday">
                    <cellValueFactory>
                        <PropertyValueFactory property="tuesday" />
                    </cellValueFactory>
                    <columns>
                        <TableColumn text="P1">
                            <cellValueFactory>
                                <PropertyValueFactory property="tp1" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn text="P2">
                            <cellValueFactory>
                                <PropertyValueFactory property="tp2" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn text="P3">
                            <cellValueFactory>
                                <PropertyValueFactory property="tp3" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn text="P4">
                            <cellValueFactory>
                                <PropertyValueFactory property="tp4" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn text="P5">
                            <cellValueFactory>
                                <PropertyValueFactory property="tp5" />
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                </TableColumn>
                <TableColumn text="Wednesday">
                    <cellValueFactory>
                        <PropertyValueFactory property="wednesday" />
                    </cellValueFactory>
                    <columns>
                        <TableColumn text="P1">
                            <cellValueFactory>
                                <PropertyValueFactory property="wp1" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn text="P2">
                            <cellValueFactory>
                                <PropertyValueFactory property="wp2" />
