                </TableColumn>
                <TableColumn text="Friday">
                    <cellValueFactory>
                        <PropertyValueFactory property="friday" />
                    </cellValueFactory>
                    <columns>
                        <TableColumn text="P1">
                            <cellValueFactory>
                                <PropertyValueFactory property="fp1" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn text="P2">
                            <cellValueFactory>
                                <PropertyValueFactory property="fp2" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn text="P3">
                            <cellValueFactory>
